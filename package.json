    },
    "files": [
        "lib",
        "dist"
    ],
    "homepage": "https://github.com/Selz/js-sdk#readme",
    "dependencies": {
        "core-js": "^2.5.1",
        "pascalcase-keys": "^1.0.1",
        "snakecase-keys": "^1.1.0"
    },
    "devDependencies": {
        "babel-core": "^6.26.0",
        "babel-eslint": "^8.0.1",
        "babel-plugin-external-helpers": "^6.22.0",
        "babel-preset-env": "^1.6.1",
        "babel-register": "^6.26.0",
        "babelrc-rollup": "^3.0.0",
        "eslint": "^4.10.0",
        "eslint-config-airbnb": "^16.1.0",
