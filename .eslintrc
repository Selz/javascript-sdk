{
    "parser": "babel-eslint",
    "extends": ["airbnb-base", "prettier"],
    "env": {
        "browser": true,
        "es6": true
    },
    "rules": {
        "indent": ["error", 4, { "SwitchCase": 1 }],
        "quotes": ["error", "single", "avoid-escape"],
        "camelcase": ["error", { "properties": "never" }],
        "no-param-reassign": [2, { "props": false }],
        "no-underscore-dangle": 0
    },
    "parserOptions": {
        "sourceType": "module"
    }
}
