{
    "name": "selz-js-sdk",
    "version": "0.1.0",
    "description": "JavaScript SDK for use with the Selz platform ",
    "main": "dist/SelzClient.js",
    "module": "dist/SelzClient.mjs",
    "jsnext:main": "dist/SelzClient.mjs",
    "directories": {
        "doc": "docs"
    },
    "scripts": {
        "prebuild": "eslint lib test",
        "build": "rollup -c --environment BUILD:production",
        "watch": "rollup -c -w",
        "pretest": "rollup -c",
        "test": "mocha",
        "prepublish": "npm test"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Selz/js-sdk.git"
    },
    "keywords": ["Selz", "SDK", "JavaScript", "Store"],
    "author": "Sam Potts <sam@selz.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Selz/js-sdk/issues"
    },
    "files": ["lib", "dist"],
    "homepage": "https://github.com/Selz/js-sdk#readme",
    "dependencies": {
        "snakecase-keys": "^1.1.0",
        "urijs": "^1.19.0"
    },
    "devDependencies": {
        "babel-eslint": "^8.0.1",
        "babel-plugin-external-helpers": "^6.22.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-register": "^6.26.0",
        "babelrc-rollup": "^3.0.0",
        "eslint": "^4.10.0",
        "eslint-config-airbnb": "^16.1.0",
        "eslint-config-prettier": "^2.6.0",
        "istanbul": "^0.4.5",
        "mocha": "^4.0.1",
        "prettier-eslint": "^8.2.1",
        "rollup": "^0.50.0",
        "rollup-plugin-babel": "^3.0.2",
        "rollup-plugin-commonjs": "^8.2.5",
        "rollup-plugin-istanbul": "^1.1.0",
        "rollup-plugin-node-resolve": "^3.0.0",
        "rollup-plugin-uglify": "^2.0.1",
        "rollup-watch": "^4.3.1",
        "uglify-es": "^3.1.6"
    }
}
